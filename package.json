  "scripts": {
    "start": "node server.js",
    "start-dev": "ng serve",
    "start-development": "ng serve",
    "start-prod": "ng serve -prod",
    "start-production": "ng serve -prod",
    "extract-translations": "ng2-translate-extract --dir ./src --output ./used_i18n.json --format=json --clean",
    "build": "ng build -${NODE_ENV-prod}",
    "build-aot": "ng build -${NODE_ENV-prod} --aot",
    "lint": "ng lint",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "ng e2e",
    "heroku-prebuild": "yarn global add @angular/cli",
    "heroku-postbuild": "ng build -prod --aot --env=${ENV_CONFIG-prod}"
  },
  "private": false,
