  "scripts": {
    "build-dev": "yarn run build:dev-client-and-server-bundles && yarn run webpack:server",
    "build:client-and-server-bundles": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --env=${ENV_CONFIG} && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --env=${ENV_CONFIG} --app 1 --output-hashing=false",
    "build:dev-client-and-server-bundles": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --env=dev && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --env=dev --app 1 --output-hashing=false",
    "build": "yarn run build:client-and-server-bundles && yarn run webpack:server",
    "e2e": "ng e2e",
    "heroku-postbuild": "yarn run build",
    "lint": "ng lint",
    "ng": "ng",
    "start-dev": "ng serve",
    "start": "node dist/server.js",
    "test": "ng test",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
